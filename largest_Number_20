package javaclosedbook;

public class largest_Number_20 {

	public static void main(String[] args) {
		int [][] array = 
			{
					{1,25,56,78,87,156,245},
					{15,65,148,225,788,45,46},
			};
	int max = 0 ;

	for(int i =0;i<array.length;i++) {
		for(int j= 0;j<array.length;j++) {
			max = array [i][0];
			if(max<array[i][j])
			{
				max = array[i][j];
				
			}
		}
		System.out.println( max +" is greatest number ");
	}
	}

	

}
